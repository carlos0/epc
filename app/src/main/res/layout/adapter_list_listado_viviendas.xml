<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardBackgroundColor="@color/color_transparent"
    app:cardCornerRadius="0dp"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
        <LinearLayout
            android:id="@+id/contenedor"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/list_encuesta_activity_bg2"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="5dp"
            android:gravity="top"
            android:elevation="3dp"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/list_it"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="3"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:layout_marginTop="40dp"
                android:paddingLeft="6dp"
                android:paddingRight="6dp">
                <TextView
                    android:id="@+id/list_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimaryDark"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:textStyle="bold" />
                <TextView
                    android:id="@+id/list_mzn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:textStyle="bold"
                    android:layout_weight="1"
                    android:layout_gravity="center"/>
                <TextView
                    android:id="@+id/lv_manzanas"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:textStyle="bold"
                    android:layout_weight="1"
                    android:gravity="right"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/list_it_lv2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/color_transparent"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingLeft="6dp"
                android:weightSum="2"
                android:paddingRight="6dp">

                <TextView
                    android:id="@+id/lv_uso"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:textColor="@color/colorPrimary_text"
                    android:background="@color/color_transparent"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/list_orden"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/colorPrimary_text"
                    android:background="@color/color_transparent"
                    android:layout_height="wrap_content" />

            </LinearLayout>
            <LinearLayout
                android:id="@+id/list_it_lv3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="vertical"
                android:paddingLeft="6dp"
                android:weightSum="2"
                android:paddingRight="6dp">

                <TextView
                    android:id="@+id/list_descripcion"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:textColor="@color/colorPrimary_text"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/list_jefe"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:textColor="@color/colorPrimary_text"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="3"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:weightSum="4"
                    android:layout_margin="2dp"
                    android:background="@drawable/list_encuesta_activity_bg2"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/count_hogar"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textSize="20sp"
                        android:textColor="@color/colorPrimary"
                        android:text="1000"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1.5"
                        android:gravity="center"
                        android:text="Hogares"
                        android:textColor="@color/colorPrimary_text"
                        android:textSize="10sp"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1.5"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@color/colorPrimary_light"
                        android:textSize="10sp"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:weightSum="4"
                    android:layout_margin="2dp"
                    android:background="@drawable/list_encuesta_activity_bg2"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/count_hombre"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textSize="20sp"
                        android:textColor="@color/colorPrimary"
                        android:text="1000"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1.5"
                        android:gravity="center"
                        android:text="Hombres"
                        android:textColor="@color/colorPrimary_text"
                        android:textSize="10sp"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1.5"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@color/colorPrimary_light"
                        android:textSize="10sp"
                        />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:weightSum="4"
                    android:layout_margin="2dp"
                    android:background="@drawable/list_encuesta_activity_bg2"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/count_mujer"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textSize="20sp"
                        android:textColor="@color/colorPrimary"
                        android:text="1000"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1.5"
                        android:gravity="center"
                        android:text="Mujeres"
                        android:textColor="@color/colorPrimary_text"
                        android:textSize="10sp"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1.5"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@color/colorPrimary_light"
                        android:textSize="10sp"
                        />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dp"
                android:background="@color/color_transparent"
                android:id="@+id/list_it_lv"
                >
                <TextView
                    android:id="@+id/list_nro_vivienda_objeto_estudio"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:textColor="@color/colorPrimaryDark"
                    android:textStyle="italic" />

            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/list_upm_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_gravity="start"
        android:elevation="5dp"
        android:background="@drawable/ic_main"
        android:orientation="vertical">

        <TextView
            android:id="@+id/lv_codigo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:textColor="@color/icons"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:textSize="20dp" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_gravity="top|start"
        android:elevation="5dp"
        android:background="@drawable/ic_main"
        android:visibility="gone"
        android:orientation="vertical">

        <TextView
            android:id="@+id/lv_manzana"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:textColor="@color/icons"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:textSize="20dp"
            android:layout_gravity="center"/>

    </LinearLayout>


    <LinearLayout
        android:id="@+id/list_predio_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_gravity="end"
        android:elevation="5dp"
        android:background="@drawable/ic_main"
        android:orientation="vertical">

        <TextView
            android:id="@+id/lv_predio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|center_horizontal"
            android:textColor="@color/icons"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:textSize="20dp" />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/list_voe_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_gravity="top|end"
        android:elevation="5dp"
        android:background="@drawable/ic_main"
        android:visibility="gone"
        android:orientation="vertical">

        <TextView
            android:id="@+id/list_voe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|center_horizontal"
            android:textColor="@color/icons"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:textSize="20dp"
            android:textStyle="bold"
            android:layout_gravity="center"/>

    </LinearLayout>

</androidx.cardview.widget.CardView>
